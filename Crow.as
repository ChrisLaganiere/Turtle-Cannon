package{	import flash.display.MovieClip;	import flash.events.Event;		public class Crow extends Cannon	{				private var _vx:Number = 20*(0.5-Math.random());		private var _vy:Number = 0;				public function Cannon()		{		}		override public function frameActions():void		{			if (Collision.checkVisible(this))			{				if (this.hitTestObject(MovieClip(parent).player))				{					Collision.crowBlock(MovieClip(parent).player, this);				}			}			boundaries();			this.x += _vx;			this.y += _vy;		}		private function boundaries():void		{						if (this.x > Main.stageW + this.width/2)			{				this.x = 0 - this.width/2;			}			else if (this.x < 0 - this.width/2)			{				this.x = Main.stageW + this.width/2;			}		}	}}